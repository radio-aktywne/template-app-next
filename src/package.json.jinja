{
  "name": "{{ appname }}",
  "private": true,
  "scripts": {
    "build": "next build",
    "clean": "rm --recursive --force build/",
    "dev": "next dev --hostname \"${ {{- envprefix }}__SERVER__HOST:-0.0.0.0}\" --port \"${ {{- envprefix }}__SERVER__PORT:-{{ port -}} }\"",
    "locales": "lingui extract --clean --overwrite",
    "run": "next start --hostname \"${ {{- envprefix }}__SERVER__HOST:-0.0.0.0}\" --port \"${ {{- envprefix }}__SERVER__PORT:-{{ port -}} }\"",
    "update": "ncu --peer --upgrade && npm i --no-audit --no-fund"
  },
  "dependencies": {
    "@lingui/react": "^5.2.0",
    "@mantine/core": "^7.17.0",
    "@mantine/hooks": "^7.17.0",
    "@mantine/notifications": "^7.17.0",
    "@tabler/icons-react": "~3.17.0",
    "client-only": "^0.0.1",
    "immer": "^10.1.1",
    "next": "14.2.24",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "server-only": "^0.0.1",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.7",
    "@eslint/eslintrc": "^3.3.0",
    "@eslint/js": "^9.21.0",
    "@lingui/cli": "^5.2.0",
    "@lingui/format-po": "^5.2.0",
    "@lingui/loader": "^5.2.0",
    "@lingui/swc-plugin": "5.0.0-next.2",
    "@next/eslint-plugin-next": "14.2.24",
    "@types/node": "^20.17.19",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "eslint-plugin-lingui": "^0.10.0",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-react": "^7.37.4",
    "globals": "^16.0.0",
    "npm-check-updates": "^17.1.14",
    "postcss": "^8.5.3",
    "postcss-preset-mantine": "^1.17.0",
    "postcss-simple-vars": "^7.0.1",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.1"
  },
  "overrides": {
    "@lingui/react": "$@lingui/react"
  }
}
